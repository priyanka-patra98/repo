{"components/common/navigation-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-parent\">\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n                        {{name}}<i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use href=\"#icon-chevron-down\" /></svg></i>\n                    </a>\n                    {{> components/common/navigation-dropdown}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n","components/common/navigation-list":"{{#if children}}\n<a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\"\n   data-collapsible=\"navPages-{{id}}\"\n>\n    {{name}}\n    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n        <svg><use href=\"#icon-chevron-down\" /></svg>\n    </i>\n</a>\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item\">\n            <a class=\"navPage-subMenu-action navPages-action\"\n               href=\"{{url}}\"\n               aria-label=\"{{lang 'category.view_all.name' category=name}}\"\n            >\n                {{lang 'category.view_all.name' category=name}}\n            </a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n                       href=\"{{url}}\"\n                       aria-label=\"{{name}}\"\n                    >\n                        {{name}}\n                        <span class=\"collapsible-icon-wrapper\"\n                            data-collapsible=\"navPages-{{id}}\"\n                            data-collapsible-disabled-breakpoint=\"medium\"\n                            data-collapsible-disabled-state=\"open\"\n                            data-collapsible-enabled-state=\"closed\"\n                        >\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                                <svg><use href=\"#icon-chevron-down\" /></svg>\n                            </i>\n                        </span>\n                    </a>\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\n                        {{#each children}}\n                        <li class=\"navPage-childList-item\">\n                            <a class=\"navPage-childList-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                               href=\"{{url}}\"\n                               aria-label=\"{{name}}\"\n                            >\n                                {{name}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                       href=\"{{url}}\"\n                       aria-label=\"{{name}}\"\n                    >\n                        {{name}}\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n{{else}}\n<a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\"\n   aria-label=\"{{name}}\"\n>\n    {{name}}\n</a>\n{{/if}}\n","components/common/navigation-list-alternate":"{{#if children}}\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       data-collapsible=\"navPages-{{id}}\"\n       aria-label=\"{{name}}\"\n    >\n        {{name}}\n        <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n            <svg><use href=\"#icon-chevron-down\" /></svg>\n        </i>\n    </a>\n    {{> components/common/navigation-dropdown}}\n{{else}}\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        {{name}}\n    </a>\n{{/if}}\n","components/common/navigation-menu":"<nav class=\"navPages\">\r\n    <div class=\"navPages-quickSearch\">\r\n        {{> components/common/quick-search name='nav-menu-quick-search'}}\r\n    </div>\r\n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\r\n        {{#each categories}}\r\n            <li class=\"navPages-item\">\r\n            {{#if ../theme_settings.navigation_design '===' 'simple'}}\r\n                {{> components/common/navigation-list}}\r\n            {{/if}}\r\n            {{#if ../theme_settings.navigation_design '===' 'alternate'}}\r\n                {{> components/common/navigation-list-alternate}}\r\n            {{/if}}\r\n            </li>\r\n        {{/each}}\r\n        {{#unless theme_settings.hide_content_navigation}}\r\n            {{#each pages}}\r\n                 <li class=\"navPages-item navPages-item-page\">\r\n                     <a class=\"navPages-action{{#if name '==' ../page.title}} activePage{{/if}}\"\r\n                        href=\"{{url}}\"\r\n                        aria-label=\"{{name}}\"\r\n                     >\r\n                         {{name}}\r\n                     </a>\r\n                     <button class=\"btn-clickme\" type=\"button\">Distributor Locator</button>\r\n                 </li>\r\n             {{/each}}\r\n        {{/unless}}\r\n    </ul>\r\n    <ul class=\"navPages-list navPages-list--user\">\r\n        {{#if currency_selector.currencies.length '>' 1}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action has-subMenu\"\r\n                   href=\"#\"\r\n                   data-collapsible=\"navPages-currency\"\r\n                   aria-controls=\"navPages-currency\"\r\n                   aria-expanded=\"false\"\r\n                   aria-label=\"{{lang 'common.currency' code=currency_selector.active_currency_code}}\"\r\n                >\r\n                    {{lang 'common.currency' code=currency_selector.active_currency_code}}\r\n                    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use href=\"#icon-chevron-down\"></use>\r\n                        </svg>\r\n                    </i>\r\n                </a>\r\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                    <ul class=\"navPage-subMenu-list\">\r\n                        {{#each currency_selector.currencies}}\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{{switch_url}}}\"\r\n                               aria-label=\"{{name}}\"\r\n                               data-currency-code=\"{{{code}}}\"\r\n                               {{#if cart_currency_switch_url.length '>' 0 }}\r\n                               data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\r\n                               {{/if}}\r\n                               data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\r\n                            >\r\n                                {{#if is_active}}\r\n                                    <strong>{{name}}</strong>\r\n                                {{else}}\r\n                                    {{name}}\r\n                                {{/if}}\r\n                            </a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                </div>\r\n            </li>\r\n        {{/if}}\r\n        {{#if settings.gift_certificates_enabled}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\"\r\n                   href=\"{{urls.gift_certificate.purchase}}\"\r\n                   aria-label=\"{{lang 'common.gift_cert'}}\"\r\n                >\r\n                    {{lang 'common.gift_cert'}}\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if customer.store_credit.value '>' 0}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action navPages-action--storeCredit\">\r\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if theme_settings.product_list_display_mode '==' 'list'}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action navPages-action--compare\"\r\n                   href=\"{{urls.compare}}\"\r\n                   data-compare-nav\r\n                   aria-label=\"{{lang 'common.compare'}}\"\r\n                >\r\n                    {{lang 'common.compare'}}\r\n                    <span class=\"countPill countPill--positive countPill--alt\"></span>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if customer}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action has-subMenu\"\r\n                   href=\"{{urls.account.index}}\"\r\n                   data-collapsible=\"navPages-account\"\r\n                   aria-label=\"{{lang 'common.account'}}\"\r\n                >\r\n                    {{lang 'common.account'}}\r\n                    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use href=\"#icon-chevron-down\"></use>\r\n                        </svg>\r\n                    </i>\r\n                </a>\r\n                <div class=\"navPage-subMenu\" id=\"navPages-account\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                    <ul class=\"navPage-subMenu-list\">\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{urls.account.orders.all}}\"\r\n                               aria-label=\"{{lang 'account.nav.orders'}}\"\r\n                            >\r\n                                {{lang 'account.nav.orders'}}\r\n                            </a>\r\n                        </li>\r\n                        {{#if settings.returns_enabled}}\r\n                            <li class=\"navPage-subMenu-item\">\r\n                                <a class=\"navPage-subMenu-action navPages-action\"\r\n                                   href=\"{{urls.account.returns}}\"\r\n                                   aria-label=\"{{lang 'account.nav.returns'}}\"\r\n                                >\r\n                                    {{lang 'account.nav.returns'}}\r\n                                </a>\r\n                            </li>\r\n                        {{/if}}\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{urls.account.inbox}}\"\r\n                               aria-label=\"{{lang 'account.mobile_nav.messages'}}\"\r\n                            >\r\n                                {{lang 'account.mobile_nav.messages'}}\r\n                            </a>\r\n                        </li>\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{urls.account.addresses}}\"\r\n                               aria-label=\"{{lang 'account.nav.addresses'}}\"\r\n                            >\r\n                                {{lang 'account.nav.addresses'}}\r\n                            </a>\r\n                        </li>\r\n                        {{#if settings.show_payment_methods}}\r\n                            <li class=\"navPage-subMenu-item\">\r\n                                <a class=\"navPage-subMenu-action navPages-action\"\r\n                                   href=\"{{urls.account.payment_methods.all}}\"\r\n                                   aria-label=\"{{lang 'account.nav.payment_methods'}}\"\r\n                                >\r\n                                    {{lang 'account.nav.payment_methods'}}\r\n                                </a>\r\n                            </li>\r\n                        {{/if}}\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{urls.account.wishlists.all}}\"\r\n                               aria-label=\"{{lang 'account.mobile_nav.wishlists'}}\"\r\n                            >\r\n                                {{lang 'account.mobile_nav.wishlists'}}\r\n                            </a>\r\n                        </li>\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{urls.account.recent_items}}\"\r\n                               aria-label=\"{{lang 'account.nav.recently_viewed'}}\"\r\n                            >\r\n                                {{lang 'account.nav.recently_viewed'}}\r\n                            </a>\r\n                        </li>\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{urls.account.details}}\"\r\n                               aria-label=\"{{lang 'account.nav.settings'}}\"\r\n                            >\r\n                                {{lang 'account.nav.settings'}}\r\n                            </a>\r\n                        </li>\r\n                    </ul>\r\n                </div>\r\n            </li>\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\"\r\n                   href=\"{{urls.auth.logout}}\"\r\n                   aria-label=\"{{lang 'common.logout'}}\"\r\n                >\r\n                    {{lang 'common.logout'}}\r\n                </a>\r\n            </li>\r\n        {{else}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\"\r\n                   href=\"{{urls.auth.login}}\"\r\n                   aria-label=\"{{lang 'common.login'}}\"\r\n                >\r\n                    {{lang 'common.login'}}\r\n                </a>\r\n                {{#if settings.account_creation_enabled}}\r\n                    {{lang 'common.or'}}\r\n                    <a class=\"navPages-action\"\r\n                       href=\"{{urls.auth.create_account}}\"\r\n                       aria-label=\"{{lang 'common.sign_up'}}\"\r\n                    >\r\n                        {{lang 'common.sign_up'}}\r\n                    </a>\r\n                {{/if}}\r\n            </li>\r\n        {{/if}}\r\n        {{#if theme_settings.social_icon_placement_top}}\r\n            <li class=\"navPages-item\">\r\n                {{> components/common/social-links}}\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n</nav>\r\n<script>\r\n    document.addEventListener('DOMContentLoaded', function() {\r\n    const dropdowns = document.querySelectorAll('.navPages-action.has-subMenu');\r\n\r\n    dropdowns.forEach(dropdown => {\r\n        dropdown.addEventListener('click', function(event) {\r\n            event.preventDefault();\r\n            const subMenu = this.nextElementSibling;\r\n\r\n            if (subMenu) {\r\n                const isExpanded = this.getAttribute('aria-expanded') === 'true';\r\n                this.setAttribute('aria-expanded', !isExpanded);\r\n                subMenu.setAttribute('aria-hidden', isExpanded);\r\n            }\r\n        });\r\n    });\r\n});\r\n\r\n</script>\r\n<style>\r\n   .icon.navPages-action-moreIcon {\r\n    display: inline-block;\r\n    width: 16px;\r\n    height: 16px;\r\n    vertical-align: middle;\r\n    fill: currentColor;\r\n}\r\n\r\nbutton.btn-clickme {\r\n    color: #ea7c54 !important;\r\n    padding: 7px 18px !important;\r\n    border: 2px solid #ea7c54 !important;\r\n    border-radius: 4px !important;\r\n    background: none !important;\r\n    font-size: 14px !important;\r\n}\r\n\r\n.navPage-subMenu {\r\n    box-shadow: 0px 2px 5px rgba(0, 0, 0, 0.2); /* Adjust shadow properties as needed */\r\n}\r\n\r\n</style>","components/common/quick-search":"<div class=\"container\">\n    <form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\"\n                       data-search-quick\n                       name=\"{{name}}\"\n                       id=\"{{name}}\"\n                       data-error-message=\"{{lang 'search.error.empty_field'}}\"\n                       placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\"\n                       autocomplete=\"off\"\n                >\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n    <p role=\"status\"\n       aria-live=\"polite\"\n       class=\"aria-description--hidden\"\n       data-search-aria-message-predefined-text=\"{{{lang 'search.results.quick_count_live'}}}\"\n    ></p>\n</div>\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\"\n                   href=\"{{url}}\"\n                   target=\"_blank\"\n                   rel=\"noopener\"\n                   title=\"{{{capitalize display_name}}}\"\n                >\n                    <span class=\"aria-description--hidden\">{{{capitalize display_name}}}</span>\n                    <svg>\n                        <use href=\"#icon-{{display_name}}\"/>\n                    </svg>\n                </a>\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n"}
