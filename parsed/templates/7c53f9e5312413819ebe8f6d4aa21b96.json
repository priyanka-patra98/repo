n            \r\n            \r\n            \r\n           <button class=\"accordion\">\r\n            <span class=\"navUser-greeting\"><img src=\"https://static-assets-web.flixcart.com/batman-returns/batman-returns/p/images/profile-52e0dc.svg\" class=\"normal_img\"/>Hi, {{first (split customer.name \" \") }} <i class=\"fas fa-chevron-down\"></i> </span>\r\n            \r\n           </button>\r\n            \r\n            \r\n            <div class=\"panel\">\r\n                <div class=\"navUser-dropdown-menu\">\r\n                    <div class=\"navUser-dropdown-item\">\r\n                        <a href=\"{{urls.account.index}}\" aria-label=\"My Account\">My Account</a>\r\n                    </div>\r\n                    <div class=\"navUser-dropdown-item\">\r\n                        <a href=\"{{urls.auth.logout}}\" aria-label=\"Logout\">Sign out</a>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n                \r\n                \r\n                \r\n            </div>\r\n        </div>\r\n    {{else}}\r\n        <a class=\"navUser-action\" href=\"{{urls.auth.login}}\" aria-label=\"{{lang 'common.login'}}\">\r\n            {{lang 'common.login'}}\r\n        </a>\r\n        {{#if settings.account_creation_enabled}}\r\n            <span class=\"navUser-or\">{{lang 'common.or'}}</span>\r\n            <a class=\"navUser-action\" href=\"{{urls.auth.create_account}}\" aria-label=\"{{lang 'common.sign_up'}}\">\r\n\r\n                sign up\r\n            </a>\r\n        {{/if}}\r\n    \r\n \r\n\r\n    {{/if}}\r\n</li>\r\n\r\n\r\n\r\n        <li class=\"navUser-item navUser-item--cart\">\r\n            <a class=\"navUser-action\"\r\n               data-cart-preview\r\n               data-dropdown=\"cart-preview-dropdown\"\r\n               data-options=\"align:right\"\r\n               href=\"{{urls.cart}}\"\r\n               aria-label=\"{{lang 'cart.nav_aria_label'}}\"\r\n            >\r\n               <span class=\"navUser-item-cartLabel\"><img src=\"https://static-assets-web.flixcart.com/batman-returns/batman-returns/p/images/header_cart-eed150.svg\" class=\"normal_img\"/></span> <span class=\"countPill cart-quantity\"></span>\r\n            </a>\r\n\r\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n        </li>\r\n    </ul>\r\n    <div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" data-prevent-quick-search-close>\r\n        {{> components/common/quick-search name='nav-quick-search'}}\r\n    </div>\r\n</nav>\r\n\r\n\r\n    <script>\r\nvar acc = document.getElementsByClassName(\"accordion\");\r\nvar i;\r\n\r\nfor (i = 0; i < acc.length; i++) {\r\n  acc[i].addEventListener(\"click\", function() {\r\n    // Remove \"active\" class from all accordion items before toggling\r\n    for (var j = 0; j < acc.length; j++) {\r\n      acc[j].classList.remove(\"active\");\r\n    }\r\n\r\n    // Toggle \"active\" class on the clicked item\r\n    this.classList.toggle(\"active\");\r\n\r\n    var panel = this.nextElementSibling;\r\n    if (panel.style.display === \"block\") {\r\n      panel.style.display = \"none\";\r\n    } else {\r\n      panel.style.display = \"block\";\r\n    }\r\n  });\r\n\r\n  // Initially set all panels to hidden (optional, but ensures none are open by default)\r\n  acc[i].nextElementSibling.style.display = \"none\";\r\n}\r\n\r\n</script>\r\n\r\n\r\n<style>\r\ninput#search_query {\r\n    width: 290px !important;\r\n}\r\n    .navUser-item>a {\r\n    color: #ea7c54 !important;\r\n    line-height: normal;\r\n    text-decoration: none !important;\r\n}\r\nbutton#search-button {\r\n   width: 13px !important;\r\n    /* height: 10px !important; */\r\n    position: absolute;\r\n    top: 12px !important;\r\n    right: 16em !important;\r\n    bottom: 210px !important;\r\n}\r\n.search-container {\r\n    position: relative !important;\r\n}\r\n    .navPages {\r\n        margin: 0 auto;\r\n        max-width: 90.71429rem !important;\r\n        \r\n    }\r\n}\r\n.navUser-dropdown-menu {\r\n  border: 1px solid #000 !important;\r\n}\r\n\r\n</style>\r\n","components/common/quick-search":"<div class=\"container\">\n    <form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\"\n                       data-search-quick\n                       name=\"{{name}}\"\n                       id=\"{{name}}\"\n                       data-error-message=\"{{lang 'search.error.empty_field'}}\"\n                       placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\"\n                       autocomplete=\"off\"\n                >\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n    <p role=\"status\"\n       aria-live=\"polite\"\n       class=\"aria-description--hidden\"\n       data-search-aria-message-predefined-text=\"{{{lang 'search.results.quick_count_live'}}}\"\n    ></p>\n</div>\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\"\n                   href=\"{{url}}\"\n                   target=\"_blank\"\n                   rel=\"noopener\"\n                   title=\"{{{capitalize display_name}}}\"\n                >\n                    <span class=\"aria-description--hidden\">{{{capitalize display_name}}}</span>\n                    <svg>\n                        <use href=\"#icon-{{display_name}}\"/>\n                    </svg>\n                </a>\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n"}
