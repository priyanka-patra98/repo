anel.style.display === \"block\") {\r\n      panel.style.display = \"none\";\r\n    } else {\r\n      panel.style.display = \"block\";\r\n    }\r\n  });\r\n\r\n  // Initially set all panels to hidden (optional, but ensures none are open by default)\r\n  acc[i].nextElementSibling.style.display = \"none\";\r\n}\r\n\r\n</script>\r\n\r\n\r\n<style>\r\ninput#search_query {\r\n    width: 290px !important;\r\n}\r\n    .navUser-item>a {\r\n    color: #ea7c54 !important;\r\n    line-height: normal;\r\n    text-decoration: none !important;\r\n}\r\nbutton#search-button {\r\n   width: 13px !important;\r\n    /* height: 10px !important; */\r\n    position: absolute;\r\n    top: 12px !important;\r\n    right: 16em !important;\r\n    bottom: 210px !important;\r\n}\r\n.search-container {\r\n    position: relative !important;\r\n}\r\n    .navPages {\r\n        margin: 0 auto;\r\n        max-width: 90.71429rem !important;\r\n        \r\n    }\r\n}\r\n.navUser-dropdown-menu {\r\n  border: 1px solid #000 !important;\r\n}\r\n\r\n</style>\r\n","components/common/navigation-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-parent\">\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n                        {{name}}<i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use href=\"#icon-chevron-down\" /></svg></i>\n                    </a>\n                    {{> components/common